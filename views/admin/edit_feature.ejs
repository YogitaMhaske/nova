<%-include('nav.ejs')%>
 <form action="/admin/save_feature" method="post" enctype="multipart/form-data">
    <div class="container-fluid bg-white p-3 border"> 
        <h3 class="mb-3 text-info"> Edit Feature </h3>
          <div class="row">
            <input type="hidden" name="feature_id" class="form-control" value="<%=edit_feat[0].feature_id %>">
            <div class="col-md-6 mb-4">
                 Enter icon
                 <input type="text" class="form-control"  name="feature_icon"  value="<%=edit_feat[0].feature_icon%>">
            </div> 
            <div class="col-md-6 mb-4">
                Enter Feature
                <input type="text" class="form-control"  name="feature"  value="<%=edit_feat[0].feature%>">
           </div>  
            <div class="col-12 mt-4">
                <button class="btn btn-primary float-right">SAVE FEATURE</button>
             </div>
             </div>
        </div>
    </form>
    <br><br>

    <%-include('footer.ejs')%>
