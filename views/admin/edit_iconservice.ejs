<%-include('nav.ejs')%>
<form action="/admin/update_icon_service" method="post" enctype="multipart/form-data">
 <div class="container-fluid bg-white p-3">
  <div class="row">
    <div class="col-12">
          <h3 class=" text-info">EDIT ICON SERVICE</h3>
    </div><br><br>
    <input type="hidden" class="form-control" name="icon_service_id" value="<%=icon_service[0].icon_service_id%>">
    <div class="col-md-6 mb-4">
        Enter Icon class
        <input type="text" class="form-control" name="service_icon" value="<%=icon_service[0].service_icon%>">
   </div>
    <div class="col-md-6 mb-4">
        Enter Service Title
        <input type="text" class="form-control" name="service_title" value="<%=icon_service[0].service_title%>">
    </div>
    <div class="col-12 mb-4">
        Enter Service Description
        <textarea class="form-control" name="service_desc"><%=icon_service[0].service_desc%></textarea>
    </div>
    <div class="col-12 text-right" >
        <button class="btn btn-primary">EDIT ICON SERVICE</button>
    </div>
  </div>
 </div>
</form>
<%-include('footer.ejs')%>