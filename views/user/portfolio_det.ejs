<%-include('nav.ejs')%>
<main id="main">

    <!-- ======= Breadcrumbs ======= -->
    <div class="breadcrumbs d-flex align-items-center" style="background-image: url(/uploads/<%=port_det_img[0].portfolio_det_image%>);">
      <div class="container position-relative d-flex flex-column align-items-center">

        <h2>Portfolio Details</h2>
        <ol>
          <li><a href="/">Home</a></li>
          <li>Portfolio Details</li>
        </ol>

      </div>
    </div><!-- End Breadcrumbs -->

    <!-- ======= Portfolio Details Section ======= -->
    <section id="portfolio-details" class="portfolio-details">
      <div class="container" data-aos="fade-up">

        <div class="row gy-4">

          <div class="col-lg-8">
            <div class="slides-1 portfolio-details-slider swiper">
              <div class="swiper-wrapper align-items-center">
       <% for (i=0;i<portfolio.length;i++) { %>
                <div class="swiper-slide">
                  <img src="/uploads/<%=portfolio[i].portfolio_image%>" alt="" height="450px" width="450ox">
                </div>
         <% } %>
                <!-- <div class="swiper-slide">
                  <img src="assets/img/portfolio/product-1.jpg" alt="">
                </div>

                <div class="swiper-slide">
                  <img src="assets/img/portfolio/branding-1.jpg" alt="">
                </div>

                <div class="swiper-slide">
                  <img src="assets/img/portfolio/books-1.jpg" alt="">
                </div> -->

              </div>
              <div class="swiper-pagination"></div>
            </div>
          </div>

          <div class="col-lg-4">
            <div class="portfolio-info">
              <h3>Project information</h3>
              <ul>
                <li><strong>Category</strong>: <%=portfolio[0].portfolio_category%></li>
                <li><strong>Client</strong>: <%=portfolio[0].portfolio_client%></li>
                <li><strong>Project date</strong>: <%=portfolio[0].portfolio_date%></li>
                <li><strong>Project URL</strong>: <a href="#"><%=portfolio[0].portfolio_url%></a></li>
              </ul>
            </div>
            <div class="portfolio-description">
              <h2>This is an example of portfolio detail</h2>
              <p><%=portfolio[0].portfolio_desc%></p>
            </div>
          </div>

        </div>

      </div>
    </section><!-- End Portfolio Details Section -->

  </main><!-- End #main -->


<%-include('footer.ejs')%>